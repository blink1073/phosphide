{
  "name": "phosphide",
  "version": "0.1.0",
  "description": "A slightly opinionated scaffolding for building plugin-based IDE-style applications.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "phosphor-command": "git://github.com/phosphorjs/phosphor-command.git",
    "phosphor-disposable": "^1.0.5",
    "phosphor-dockpanel": "^0.9.2",
    "phosphor-menus": "^0.9.5",
    "phosphor-plugins": "^0.2.0",
    "phosphor-signaling": "^1.1.2",
    "phosphor-tabs": "^0.9.7",
    "phosphor-topsort": "^0.9.1",
    "phosphor-widget": "^0.9.13"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "expect.js": "^0.3.1",
    "glob-copy": "^0.1.0",
    "istanbul": "^0.3.19",
    "mkdirp": "^0.5.1",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.2",
    "steal": "^0.12.1",
    "typedoc": "^0.3.11",
    "typescript": "^1.6.2"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build:src": "tsc --project src",
    "build": "npm run build:src && node scripts/copycss.js",
    "prepublish": "npm run build",
    "postinstall": "npm dedupe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phosphorjs/phosphide.git"
  },
  "files": [
    "lib/*/*.js",
    "lib/*/*.d.ts",
    "lib/*/*.css",
    "lib/*.js",
    "lib/*.d.ts",
    "lib/*.css"
  ],
  "keywords": [
    "plugin",
    "application"
  ],
  "phosphor-plugin": {
    "extensionPoints": [
      {
        "main": "lib/menus/index",
        "id": "menus:main",
        "factory": "createMenuReceiver"
      },
      {
        "main": "lib/ui/index",
        "id": "ui:main",
        "factory": "createUIReceiver"
      },
      {
        "main": "lib/commands/index",
        "id": "commands:main",
        "factory": "createCommandReceiver"
      },
      {
        "main": "lib/css/index",
        "id": "phosphide:css:main",
        "factory": "createCssReceiver"
      }
    ]
  },
  "author": "Dave Willmer <dave.willmer@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/phosphorjs/phosphide/issues"
  },
  "homepage": "https://github.com/phosphorjs/phosphide"
}
